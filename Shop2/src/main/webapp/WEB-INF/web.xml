<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>aboutUs</servlet-name>
        <jsp-file>/aboutUs.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>aboutUs</servlet-name>
        <url-pattern>/aboutUs</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>productPage</servlet-name>
        <jsp-file>/productPage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>productPage</servlet-name>
        <url-pattern>/productPage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <jsp-file>/Registration.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogIn</servlet-name>
        <jsp-file>/LogIn.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogIn</servlet-name>
        <url-pattern>/LogIn</url-pattern>
    </servlet-mapping>
</web-app>